<% content_for :page_title do %>
  <%= Spree.t(:new_serviceable_zipcode) %>
<% end %>

<% content_for :page_actions do %>
  <%= link_to_with_icon 'arrow-left', Spree.t(:back_to_resource_list, resource: Spree::ServiceableZipcode.model_name.human), admin_serviceable_zipcodes_path, :class => 'btn btn-primary' %>
<% end %>

<%= render :partial => 'spree/admin/shared/error_messages', :locals => { :target => @serviceable_zipcodes } %>

<%= form_for [:admin, @serviceable_zipcode] do |f| %>
  <fieldset>
    <%= render :partial => 'form', :locals => { :f => f } %>
    <%= render :partial => 'spree/admin/shared/new_resource_links' %>
  </fieldset>
<% end %>
